@model List<JqueryExamples.Models.CustomControlProperty>
@{
    ViewBag.Title = "DynTxtCreateSecondMethod";
}

<h2>DynTxtCreateSecondMethod</h2>


<form action="~/Default/DynTxtCreateUsingDb" method="post" style="">
   
     @foreach (var item in Model)
    {
        <div class="form-inline">
            <div class="form-group col-md-2">
                <label class="control-label">@item.PropertyName</label>
            </div>
            <div class="col-md-10">
                <input type="@item.FieldType" name="@item.PropertyName" id="@item.PropertyName" value="@item.PropertyValue" class="form-control" maxlength="10"/>
            </div>
        </div>
    }
    <input type="submit" class="btn btn-success" value="Save" />
</form>

<script src="~/scripts/jquery-1.10.2.js"></script> 
<script src="~/scripts/Calculation.js"></script>